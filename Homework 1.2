!DOCTYPE html>
<script>
'use strict';
const sumOfTwo =(arr, target)=>{
const result = [];
for (let i=0; i < arr.length; i++) {
for (let j=i+1; j < arr.length; j++) {
if(arr[i] + arr[j] === target);{
result.push(i);
result.push(j);
   }
  }
 }
return result;
}
alert (sumOfTwo( [26, 3, 2, 4, 7, 1], 6) );
</script>
